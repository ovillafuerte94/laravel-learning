{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "require": {
        "php": "^8.0.2",
        "anhskohbo/no-captcha": "^3.4",
        "barryvdh/laravel-debugbar": "^3.6",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "monicahq/laravel-cloudflare": "^3.2",
        "predis/predis": "^1.1",
        "protonemedia/laravel-eloquent-where-not": "^1.2",
        "rap2hpoutre/laravel-log-viewer": "^2.1",
        "renatomarinho/laravel-page-speed": "dev-master",
        "spatie/laravel-backup": "^8.0.2",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^5.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.6",
        "imanghafoori/laravel-anypass": "dev-master",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fix-style": "@php ./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes",
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "composer dumpautoload -o"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ]
    }
}
